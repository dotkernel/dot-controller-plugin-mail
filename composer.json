{
  "name": "dotkernel/dot-controller-plugin-mail",
  "type": "library",
  "description": "DotKernel mail controller plugin component",
  "license": "MIT",
  "authors": [
    {
      "name": "n3vrax",
      "email": "tibi@apidemia.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "zendframework/zend-servicemanager": "^3.3.0",

    "dotkernel/dot-controller": "^0.3",
    "dotkernel/dot-mail": "^0.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "squizlabs/php_codesniffer": "^2.3"
  },
  "autoload": {
    "psr-4": {
      "Dot\\Controller\\Plugin\\Mail\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DotTest\\Controller\\Plugin\\Mail\\": "test/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.2-dev",
      "dev-develop": "0.3-dev"
    }
  }
}
